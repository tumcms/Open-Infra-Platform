@inbook{vilgertshofer:2017:iwcce,
    author = {S. Vilgertshofer and J. Amann and B. Willenborg and A. Borrmann and T. H. Kolbe },
    title = {Linking BIM and GIS Models in Infrastructure by Example of IFC and CityGML},
    booktitle = {Computing in Civil Engineering 2017},
    pages = {133-140},
    doi = {10.1061/9780784480823.017},
    URL = {https://ascelibrary.org/doi/abs/10.1061/9780784480823.017},
    eprint = {https://ascelibrary.org/doi/pdf/10.1061/9780784480823.017},
    abstract = { This paper demonstrates how linked data can be used to provide a semantically rich connection between the domains of building information modeling (BIM) and geographic information systems (GIS). Expertise from both domains is necessary for the planning of infrastructure projects such as roads, tunnels, bridges, and railways, as these projects usually require the consideration of widely diverging scales. As BIM and GIS rely on different types of data modeling standards, a conversion between the data models will inevitably result in data loss. To overcome this problem, we propose to utilize the concept of Linked Data which allows the original data to coexist and provide coherence by establishing references between the corresponding entities of both standards. The approach is illustrated using exemplary models of shield tunnels in both, the IFC and the CityGML format. }
}

